# Define a matrix
matrix = [[1, 2, 3], 
          [4, 5, 6], 
          [7, 8, 9]]

# Transpose the matrix using list comprehension
transpose = [[row[i] for row in matrix] for i in range(len(matrix[0]))]

# Print the transposed matrix
print("Original Matrix:")
for row in matrix:
    print(row)

print("\nTransposed Matrix:")
for row in transpose:
    print(row)
